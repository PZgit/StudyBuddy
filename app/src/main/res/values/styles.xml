<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/fhws_primary_material_light</item>
        <item name="colorPrimaryDark">@color/fhws_primary_dark_material_light</item>
        <item name="colorAccent">@color/fhws_accent_material_light</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="FHWS_light" parent="android:Theme.Material.Light.NoActionBar" >
        <item name="android:colorPrimaryDark">@color/fhws_primary_dark_material_light</item>
        <item name="android:colorAccent">@color/fhws_accent_material_light</item>
        <item name="android:colorPrimary">@color/fhws_primary_material_light</item>
    </style>

</resources>
